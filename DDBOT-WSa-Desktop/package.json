{
  "name": "ddbot-wsa-desktop",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "tauri": "tauri",
    "dev:tauri": "tauri dev",
    "build:tauri": "tauri build",
    "dev:backend": "cd backend && cargo run",
    "dev:both": "concurrently \"npm run dev\" \"npm run dev:backend\"",
    "build": "vite build",
    "build:frontend": "vite build",
    "build:backend": "cd backend && cargo build --release",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,.tsx,.vue src/",
    "service:install": "powershell -ExecutionPolicy Bypass -File ./scripts/install-service.ps1",
    "service:uninstall": "powershell -ExecutionPolicy Bypass -File ./scripts/uninstall-service.ps1"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.10.1",
    "@tauri-apps/plugin-dialog": "^2.6.0",
    "@tauri-apps/plugin-opener": "^2.5.3",
    "@tauri-apps/plugin-shell": "^2.0.1",
    "lucide-vue-next": "^0.475.0",
    "pinia": "^3.0.4",
    "vue": "^3.5.27",
    "vue-router": "^5.0.2",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.10.0",
    "@types/node": "^24.3.0",
    "@vitejs/plugin-vue": "^6.0.4",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.7.0",
    "typescript": "^5.8.3",
    "vite": "^7.3.1",
    "vue-tsc": "^3.2.4"
  }
