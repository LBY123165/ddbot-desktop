{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "DDBOT-WSa Desktop",
  "version": "0.1.0",
  "identifier": "icu.tane.ddbot.wsa.desktop",
  "build": {
    "beforeDevCommand": "yarn run dev",
    "devUrl": "http://localhost:5173",
    "beforeBuildCommand": "yarn run web:build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "DDBOT-WSa Desktop",
        "width": 1400,
        "height": 900,
        "minWidth": 900,
        "minHeight": 600,
        "resizable": true,
        "center": true,
        "decorations": true,
        "visible": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "icon": ["icons/icon.ico", "icons/icon.png"],
    "resources": []
  },
  "plugins": {}
}
